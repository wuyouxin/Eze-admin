clearfix()
  &:after
    display table
    clear both
    content ' '
scrollBar()
  &::-webkit-scrollbar-track-piece
    background #d3dce6
  &::-webkit-scrollbar
    width 6px
  &::-webkit-scrollbar-thumb
    border-radius 20px
    background #99a9bf
relative()
  position relative
  width 100%
  height 100%
